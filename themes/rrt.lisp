(in-package :lem-user)

(define-color-theme "rrt" ()
  (foreground "#FFFFFF")
  (background "#000000")
  (syntax-warning-attribute :foreground nil :background "#000000")
  (syntax-string-attribute :foreground "#87d7ff" :background "#000000")
  (syntax-comment-attribute :foreground "#00ff00" :background "#000000")
  (syntax-keyword-attribute :foreground "#ff0000" :background "#000000")
  (syntax-constant-attribute :foreground "#87ffd7" :background "#000000")
  (syntax-function-name-attribute :foreground "#ffff00" :background "#000000")
  (syntax-variable-attribute :foreground "#ffd787" :background "#000000")
  (syntax-type-attribute :foreground "#ff87ff" :background "#000000")
  (syntax-builtin-attribute :foreground nil :background "#000000"))
