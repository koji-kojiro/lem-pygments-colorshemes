(in-package :lem-user)

(define-color-theme "fruity" ()
  (foreground "#ffffff")
  (background "#111111")
  (syntax-warning-attribute :foreground nil :background "#111111")
  (syntax-string-attribute :foreground "#0086d2" :background "#111111")
  (syntax-comment-attribute :foreground "#008800" :background "#111111")
  (syntax-keyword-attribute :foreground "#fb660a" :background "#111111")
  (syntax-constant-attribute :foreground "#0086d2" :background "#111111")
  (syntax-function-name-attribute :foreground "#ff0086" :background "#111111")
  (syntax-variable-attribute :foreground "#fb660a" :background "#111111")
  (syntax-type-attribute :foreground "#cdcaa9" :background "#111111")
  (syntax-builtin-attribute :foreground nil :background "#111111"))
