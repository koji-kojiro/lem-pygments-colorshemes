(in-package :lem-user)

(define-color-theme "native" ()
  (foreground "#ffffff")
  (background "#202020")
  (syntax-warning-attribute :foreground "#a61717" :background "#202020")
  (syntax-string-attribute :foreground "#ed9d13" :background "#202020")
  (syntax-comment-attribute :foreground "#999999" :background "#202020")
  (syntax-keyword-attribute :foreground "#6ab825" :background "#202020")
  (syntax-constant-attribute :foreground "#40ffff" :background "#202020")
  (syntax-function-name-attribute :foreground "#447fcf" :background "#202020")
  (syntax-variable-attribute :foreground "#40ffff" :background "#202020")
  (syntax-type-attribute :foreground nil :background "#202020")
  (syntax-builtin-attribute :foreground "#24909d" :background "#202020"))
