(in-package :lem-user)

(define-color-theme "default" ()
  (foreground "#000000")
  (background "#f8f8f8")
  (syntax-warning-attribute :foreground nil :background "#f8f8f8")
  (syntax-string-attribute :foreground "#BA2121" :background "#f8f8f8")
  (syntax-comment-attribute :foreground "#408080" :background "#f8f8f8")
  (syntax-keyword-attribute :foreground "#008000" :background "#f8f8f8")
  (syntax-constant-attribute :foreground "#880000" :background "#f8f8f8")
  (syntax-function-name-attribute :foreground "#0000FF" :background "#f8f8f8")
  (syntax-variable-attribute :foreground "#19177C" :background "#f8f8f8")
  (syntax-type-attribute :foreground "#B00040" :background "#f8f8f8")
  (syntax-builtin-attribute :foreground "#008000" :background "#f8f8f8"))
