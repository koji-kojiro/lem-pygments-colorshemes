(in-package :lem-user)

(define-color-theme "tango" ()
  (foreground "#000000")
  (background "#f8f8f8")
  (syntax-warning-attribute :foreground "#a40000" :background "#f8f8f8")
  (syntax-string-attribute :foreground "#4e9a06" :background "#f8f8f8")
  (syntax-comment-attribute :foreground "#8f5902" :background "#f8f8f8")
  (syntax-keyword-attribute :foreground "#204a87" :background "#f8f8f8")
  (syntax-constant-attribute :foreground "#000000" :background "#f8f8f8")
  (syntax-function-name-attribute :foreground "#000000" :background "#f8f8f8")
  (syntax-variable-attribute :foreground "#000000" :background "#f8f8f8")
  (syntax-type-attribute :foreground "#204a87" :background "#f8f8f8")
  (syntax-builtin-attribute :foreground "#204a87" :background "#f8f8f8"))
