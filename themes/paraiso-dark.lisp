(in-package :lem-user)

(define-color-theme "paraiso-dark" ()
  (foreground "#e4e4e4")
  (background "#262626")
  (syntax-warning-attribute :foreground "#ff5f5f" :background "#262626")
  (syntax-string-attribute :foreground "#5faf87" :background "#262626")
  (syntax-comment-attribute :foreground "#767676" :background "#262626")
  (syntax-keyword-attribute :foreground "#875faf" :background "#262626")
  (syntax-constant-attribute :foreground "#ff5f5f" :background "#262626")
  (syntax-function-name-attribute :foreground "#00afff" :background "#262626")
  (syntax-variable-attribute :foreground "#ff5f5f" :background "#262626")
  (syntax-type-attribute :foreground "#ffd700" :background "#262626")
  (syntax-builtin-attribute :foreground nil :background "#262626"))
