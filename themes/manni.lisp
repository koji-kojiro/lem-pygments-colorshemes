(in-package :lem-user)

(define-color-theme "manni" ()
  (foreground "#000000")
  (background "#f0f3f3")
  (syntax-warning-attribute :foreground "#AA0000" :background "#f0f3f3")
  (syntax-string-attribute :foreground "#CC3300" :background "#f0f3f3")
  (syntax-comment-attribute :foreground "#0099FF" :background "#f0f3f3")
  (syntax-keyword-attribute :foreground "#006699" :background "#f0f3f3")
  (syntax-constant-attribute :foreground "#336600" :background "#f0f3f3")
  (syntax-function-name-attribute :foreground "#CC00FF" :background "#f0f3f3")
  (syntax-variable-attribute :foreground "#003333" :background "#f0f3f3")
  (syntax-type-attribute :foreground "#007788" :background "#f0f3f3")
  (syntax-builtin-attribute :foreground "#336666" :background "#f0f3f3"))
