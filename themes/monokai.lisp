(in-package :lem-user)

(define-color-theme "monokai" ()
  (foreground "#f8f8f2")
  (background "#272822")
  (syntax-warning-attribute :foreground "#960050" :background "#272822")
  (syntax-string-attribute :foreground "#e6db74" :background "#272822")
  (syntax-comment-attribute :foreground "#75715e" :background "#272822")
  (syntax-keyword-attribute :foreground "#66d9ef" :background "#272822")
  (syntax-constant-attribute :foreground "#66d9ef" :background "#272822")
  (syntax-function-name-attribute :foreground "#a6e22e" :background "#272822")
  (syntax-variable-attribute :foreground nil :background "#272822")
  (syntax-type-attribute :foreground nil :background "#272822")
  (syntax-builtin-attribute :foreground nil :background "#272822"))
